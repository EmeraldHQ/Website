		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/eslint": "^8.56.5",
