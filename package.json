{
	"name": "emerald-website",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"dev-exposed": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"deps:install": "pnpm i",
		"deps:update": "pnpm up",
		"deps:update-latest": "pnpm up -L",
		"check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"ci:update-inlang": "node ./scripts/update-inlang-config.js",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@inlang/sdk-js": "^0.11.8",
		"@inqling/svelte-icons": "^4.0.2",
		"@rgossiaux/svelte-headlessui": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.25.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/node": "^20.6.1",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.1",
		"husky": "^8.0.3",
		"mini-svg-data-uri": "^1.4.4",
		"postcss": "^8.4.29",
		"postcss-import": "^15.1.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.1",
		"svelte-meta-tags": "^3.0.3",
		"tailwindcss": "^3.3.3",
		"tailwindcss-3d": "^1.0.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9"
	},
	"type": "module"
}
