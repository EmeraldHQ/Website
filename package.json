{
	"name": "renew-website",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"dev-exposed": "vite --host",
		"build": "vite build",
		"preview": "vite preview",
		"deps:install": "pnpm i",
		"deps:update": "pnpm up",
		"deps:update-latest": "pnpm up -L",
		"check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"ci:lint": "prettier --check ./src && eslint ./src",
		"ci:update-inlang": "node ./scripts/update-inlang-config.js"
	},
	"devDependencies": {
		"@inlang/sdk-js": "^0.6.3",
		"@inqling/svelte-icons": "^3.3.2",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.21.0",
		"@types/node": "^20.2.5",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.2",
		"mini-svg-data-uri": "^1.4.4",
		"postcss": "^8.4.24",
		"postcss-import": "^15.1.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"svelte": "^4.0.1",
		"svelte-check": "^3.4.4",
		"svelte-meta-tags": "^2.8.0",
		"tailwindcss": "^3.3.2",
		"tailwindcss-3d": "^0.2.3",
		"tslib": "^2.5.2",
		"typescript": "^5.1.3",
		"vite": "^4.3.9"
	},
	"type": "module"
}
